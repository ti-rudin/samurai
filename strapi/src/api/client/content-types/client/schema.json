{
  "kind": "collectionType",
  "collectionName": "clients",
  "info": {
    "singularName": "client",
    "pluralName": "clients",
    "displayName": "Client"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "address": {
      "type": "text"
    },
    "registrationDate": {
      "type": "date"
    },
    "cars": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::car.car",
      "mappedBy": "client"
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "client"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "client"
    },
    "verified": {
      "type": "boolean",
      "default": false
    }
  }
}
